{
    "prefix": " ",
    "dataset": "imagenetr",
    "memory_size": 0,
    "memory_per_class": 0,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 40,
    "increment": 40,
    "model_name": "clip4dual",
    "base_method": "dual",
    "backbone_type": "vit_base_patch16_224_clip_prompt_dual",
    "get_original_backbone": true,
    "device": ["7"],
    "seed": 43,
    "if_tsne": false,
    "rt_weight": false,

    "epochs": 10,
    "init_lr": 0.003,
    "batch_size": 24,
    "weight_decay": 0,
    "min_lr": 1e-5,
    "optimizer": "adam",
    "grad_clip": 1.0,
    "scheduler": "constant",
    "reinit_optimizer": true,

    "global_pool": "token",
    "head_type": "token_with_prompt",
    "freeze": ["blocks", "patch_embed", "cls_token", "norm", "pos_embed"],

    "pretrained": true,
    "drop": 0.0,
    "drop_path": 0.0,

    "use_g_prompt": true,
    "g_prompt_length": 5,
    "g_prompt_layer_idx": [0, 1],
    "use_prefix_tune_for_g_prompt": false,

    "use_e_prompt": true,
    "e_prompt_layer_idx": [2, 3, 4],
    "use_prefix_tune_for_e_prompt": false,

    "prompt_pool": true,
    "size": 10,
    "length": 5,
    "top_k": 5,
    "initializer": "uniform",
    "prompt_key": true,
    "prompt_key_init": "uniform",
    "use_prompt_mask": true,
    "shared_prompt_pool": true,
    "shared_prompt_key": false,
    "batchwise_prompt": true,
    "embedding_key": "cls",
    "predefined_key": "",
    "pull_constraint": true,
    "pull_constraint_coeff": 0.1,
    "same_key_value": false,
    "alpha": 0.5,
    "beta": 0.5,
    "mask_ratio": 0.75,
    "tau": 1,
    "text_encoder": "clip"
}