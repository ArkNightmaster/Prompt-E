{
    "prefix": " ",
    "dataset": "domainnet",
    "memory_size": 0,
    "memory_per_class": 0,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 69,
    "increment": 69,
    "model_name": "clip_pe",
    "backbone_type": "ViT_B_16",
    "get_original_backbone": true,
    "device": ["1"],
    "seed": 1993,
    "if_tsne": false,
    "rt_weight": false,

    "epochs": 10,
    "init_lr": 0.001,
    "batch_size": 24,
    "weight_decay": 0,
    "min_lr": 1e-5,
    "optimizer": "adam",
    "grad_clip": 1.0,
    "scheduler": "cosine",
    "reinit_optimizer": true,

    "global_pool": "token",
    "head_type": "token_with_prompt",
    "freeze": ["positional_embedding", "class_embedding", "proj", "conv1", "ln_pre", "ln_post", "transformer"],

    "pretrained": true,
    "drop": 0.0,
    "drop_path": 0.0,

    "prompt_pool": true,
    "size": 10,
    "length": 12,
    "top_k": 5,
    "initializer": "uniform",
    "prompt_key": true,
    "prompt_key_init": "uniform",
    "use_prompt_mask": false,
    "shared_prompt_pool": false,
    "shared_prompt_key": false,
    "batchwise_prompt": true,
    "embedding_key": "cls",
    "predefined_key": "",
    "pull_constraint": true,
    "pull_constraint_coeff": 0.1,
    "alpha": 1.0,
    "beta": 1.0,
    "mask_ratio": 0.75,
    "text_encoder": "clip",
    "use_lrg": false,
    "tau": 1.0
}
